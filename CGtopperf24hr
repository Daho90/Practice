function main() {
    // CoinGecko API endpoint for top 500 cryptocurrency data
    const url = 'https://api.coingecko.com/api/v3/coins/markets';
    const params = '?vs_currency=usd&order=market_cap_desc&per_page=500&page=1&sparkline=false&price_change_percentage=24h';
    
    // Fetch data from CoinGecko
    const response = sup.fetch(url + params, {
        method: 'GET',
        headers: {
            'Accept': 'application/json'
        }
    });

    if (!response.ok) {
        return "Could not fetch data from CoinGecko. Please try again later.";
    }

    // Parse JSON response
    const coins = response.json();

    // Sort coins by 24hr price change percentage
    const topPerformers = coins.sort((a, b) => b.price_change_percentage_24h - a.price_change_percentage_24h).slice(0, 5);

    // Create a formatted response with the top performers
    const responseText = topPerformers.map((coin, index) => {
        return `${index + 1}. ${coin.name} (${coin.symbol.toUpperCase()}): ${coin.price_change_percentage_24h.toFixed(2)}%`;
    }).join('\n');

    return `Top 5 Performing Cryptocurrencies in the Past 24 Hours:\n${responseText}`;
}
